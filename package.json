{
  "name": "openbrewerydb-fastify",
  "version": "0.0.1",
  "description": "OpenBreweryDB REST API built with Fastify (TypeScript)",
  "type": "module",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "tap \"test/**/*.test.js\"",
    "build": "npm run clean && tsc -p tsconfig.json",
    "clean": "rimraf dist",
    "start": "npm run build && node ./dist/index.js",
    "dev": "fastify start -w -l info -P index.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "prettier": {},
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/sensible": "^4.1.0",
    "@trpc/server": "^10.12.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^4.4.0",
    "fastify-plugin": "^3.0.0",
    "zod": "^3.20.6"
  },
  "devDependencies": {
    "@types/node": "^18.14.1",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
